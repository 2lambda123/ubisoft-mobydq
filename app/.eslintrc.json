{
  "extends": [ "eslint:recommended", "plugin:react/recommended", "plugin:jest/recommended" ],
  "plugins": [ "react", "jest" ],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
     "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}